import java.util.*;
public class Main{
   public static void main(String[] args){
      Scanner sc=new Scanner(System.in);
      int x=sc.nextInt();
      double y=sc.nextDouble();
      if(x%5==0 && y>=(double)x+0.50){
      double ans=(y-x)-0.50d;
       System.out.printf("%.2f",ans);
      }
      else{
         System.out.printf("%.2f",y);
      }
   }
}
